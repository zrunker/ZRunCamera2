<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:dither="true"
            android:shape="rectangle">
            <stroke
                android:width="8dp"
                android:color="#000000" />
            <corners android:radius="100dp" />
        </shape>
    </item>

    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="6dp"
                android:color="#AAFFFFFF" />
            <corners android:radius="92dp" />
        </shape>
    </item>

    <item
        android:bottom="17dp"
        android:left="17dp"
        android:right="17dp"
        android:top="17dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
            <corners android:radius="83dp" />
        </shape>
    </item>
</layer-list>